<application>
  <component name="PhpCodeSniffer">
    <phpcs_settings>
      <PhpCSConfiguration standards="Drupal;DrupalPractice;MySource;PEAR;PHPCS;PSR1;PSR2;Squiz;Zend" tool_path="C:/PHP/phpcs" />
    </phpcs_settings>
  </component>
  <component name="PhpSmartKeysConfiguration">
    <option name="findUsagesOfBaseMethod" value="true" />
    <option name="promptFindUsagesOfBaseMethod" value="false" />
  </component>
</application>