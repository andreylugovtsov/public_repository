      <option name="FOREGROUND" value="ff6b09" />
      <option name="BACKGROUND" value="bcffe1" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <filter name="cito">
      <pattern index="2" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="tagTreeHighlightingEnabled" value="false" />
  </component>
