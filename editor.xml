<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Hack" />
    <option name="LINE_SPACING" value="1.1" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledHintProviderIds">
      <set>
        <option value="JavaScript.js.type.hints" />
      </set>
    </option>
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\b\@todo\b.*" />
  </component>
</application>